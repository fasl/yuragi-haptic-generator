# Yuragi Haptic Generator Frontend - Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_WS_BASE_URL=ws://localhost:8000

# App Configuration
VITE_APP_TITLE=Yuragi Haptic Generator
VITE_APP_DESCRIPTION=Sawtooth wave-based haptic feedback system
VITE_APP_VERSION=0.1.0

# Development Configuration
VITE_DEV_TOOLS=true
VITE_ENABLE_LOGGING=true
VITE_LOG_LEVEL=debug

# WebSocket Configuration
VITE_WS_RECONNECT_ATTEMPTS=5
VITE_WS_RECONNECT_INTERVAL=3000

# Chart Configuration
VITE_CHART_UPDATE_INTERVAL=100
VITE_CHART_MAX_DATA_POINTS=1000

# Performance Configuration
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_BUNDLE_ANALYSIS=false

# Feature Flags
VITE_ENABLE_E2E_TESTING=false
VITE_ENABLE_ANALYTICS=false