# Production Environment Configuration

# API Configuration
VITE_API_BASE_URL=https://api.yuragi-haptic.com
VITE_WS_BASE_URL=wss://api.yuragi-haptic.com

# App Configuration
VITE_APP_TITLE=Yuragi Haptic Generator
VITE_APP_DESCRIPTION=Sawtooth wave-based haptic feedback system
VITE_APP_VERSION=0.1.0

# Development Configuration
VITE_DEV_TOOLS=false
VITE_ENABLE_LOGGING=false
VITE_LOG_LEVEL=error

# WebSocket Configuration
VITE_WS_RECONNECT_ATTEMPTS=10
VITE_WS_RECONNECT_INTERVAL=5000

# Chart Configuration
VITE_CHART_UPDATE_INTERVAL=100
VITE_CHART_MAX_DATA_POINTS=500

# Performance Configuration
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_BUNDLE_ANALYSIS=false

# Feature Flags
VITE_ENABLE_E2E_TESTING=false
VITE_ENABLE_ANALYTICS=true